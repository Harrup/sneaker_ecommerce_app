<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>Hello Adonis</title>
  <link rel="stylesheet" href="/css/styles.css">
</head>
<body>
  <header>
    <div class="logo">TRAINER WORLD</div>
    <div class="menu">
      <a href=#>about</a>
      <a href=#>new arrival</a>
      <a href=#>categories</a>
      <a class="cart-link" href=#>cart<span>5</span></a>
    </div>
  </header>

  <section class="content-area home-page">
    <div class="top-section">
      <div class="product">
        <div class="circle">
          <img src="img/products/adidas-yeezy-boost-white.png">
        </div>
      </div>
      <div class="titles">
        <h3>Yeezy</h3>
        <h1>Boost 350</h1>
      </div>
    </div>
  </section>

  <section id="cart-popup" class="active">
    <div>
      <div class="cart-title">
        My cart
      </div>
    </div>
    <div class="cart-items">
      <div class="item">
        <img src="img/products/adidas-yeezy-boost-white.png">
        <div class="delete-btn">
          <div class="circle">X</div>
        </div>
      </div>
    </div>
    <div class="total-products">
      <div class="total">Total</div>
      <div class="number">X4</div>
    </div>
    <div class="total-price">
      <div class="total">Total</div>
      <div class="number">$2000</div>
    </div>
  </section>


  <script src="js/dist/vendors~FirstComp.js"></script>
  <script src="js/dist/FirstComp.js"></script>
</body>
</html>
